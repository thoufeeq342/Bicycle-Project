<?xml version="1.0"?>
<!--
/**
 * Config for Content Security Policies
 */
-->
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="script-src">
            <values>
                <value id="vimeo_script_src" type="host">player.vimeo.com</value>
                <value id="youtube_script_src" type="host">*.youtube.com</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
